{
  "name": "@plus-ui/components",
  "version": "1.0.0",
  "author": "Eqian",
  "license": "MIT",
  "description": "a component library for Vue 3",
  "main": "es/index.js",
  "unpkg": "umd/index.js",
  "jsdelivr": "umd/index.js",
  "web-types": "types/web-types.json",
  "types": "types/index.d.ts",
  "style": "lib/index.css",
  "vetur": {
    "tags": "types/tags.json",
    "attributes": "types/attributes.json"
  },
  "sideEffects": [
    "lib/*.css",
    "es/**/*.css",
    "umd/*.css"
  ],
  "keywords": [
    "vue",
    "vue-component",
    "component-library",
    "plus-ui",
    "plus-ui"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./es/index.js"
      }
    },
    "./lib/": "./lib/",
    "./es/": "./es/"
  },
  "scripts": {
    "build": "npm run build:all && npm run build:es && npm run build:umd && npm run build:dts",
    "build:all": "vite build --emptyOutDir --config ./script/build.all.ts",
    "build:es": "vite build --emptyOutDir --config ./script/build.es.ts",
    "build:umd": "vite build --emptyOutDir --config ./script/build.umd.ts",
    "build:dts": "tsx --tsconfig ./tsconfig.build.json ./script/build.dts.ts",
    "test": "vitest run"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4"
  },
  "dependencies": {
    "@types/qrcode": "1.5.0",
    "@vueuse/core": "8.7.3",
    "cropperjs": "^1.5.12",
    "jsbarcode": "3.11.5",
    "vue-router": "4.3.2",
    "co-utils-vue": "^2.4.0",
    "element-plus": "^2.7.3",
    "qrcode": "1.5.0"
  },
  "files": [
    "lib",
    "es",
    "umd",
    "types"
  ],
  "browserslist": [
    "current node",
    "last 2 versions and > 2%",
    "ie > 10"
  ]
}
