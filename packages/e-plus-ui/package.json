{
  "name": "e-plus-ui",
  "version": "1.6.0-beta.3",
  "author": "Eqian",
  "license": "MIT",
  "description": "一个基于element-plus的封装组件库，高效、简单",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "unpkg": "umd/index.js",
  "jsdelivr": "umd/index.js",
  "types": "types/index.d.ts",
  "style": "styles/index.css",
  "vetur": {
    "tags": "types/tags.json",
    "attributes": "types/attributes.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/c-eqian/e-plus-ui.git"
  },
  "sideEffects": [
    "lib/*.css",
    "es/**/*.css",
    "umd/*.css",
    "styles/*.css"
  ],
  "keywords": [
    "vue",
    "vue-component",
    "component-library",
    "e-plus-ui",
    "e-plus-ui"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.js"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./lib/": "./lib/",
    "./es/": "./es/",
    "./*": "./*",
    "./types/*": [
      "./types/*.d.ts"
    ]
  },
  "peerDependencies": {
    "element-plus": ">=2.7.6",
    "vue": ">=3.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@e-plus-ui/icons": "^1.3.1",
    "@eqian/utils-vue": "^3.1.0"
  },
  "browserslist": [
    "current node",
    "last 2 versions and > 2%",
    "ie > 10"
  ],
  "web-types": "web-types.json"
}
