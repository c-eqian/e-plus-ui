{
  "name": "plus-ui",
  "private": true,
  "version": "1.6.0-beta.3",
  "type": "module",
  "scripts": {
    "dev": "pnpm -C playground dev",
    "build": "tsx script/build/build.ci.ts",
    "dev:docs": "pnpm -C docs dev",
    "build:docs": "pnpm -C docs docs:build",
    "build:create": "pnpm -C create-template build",
    "dev:create": "pnpm -C create-template dev",
    "build:all": "vite build --emptyOutDir --config script/build/build.lib.ts",
    "build:es": "vite build --emptyOutDir --config script/build/build.es.ts",
    "build:style": "vite build --emptyOutDir --config script/build/build.styles.ts",
    "build:dts": "tsx script/build/build.dts.ts",
    "gen:global": "tsx script/build/gen.global.ts",
    "build:gulp": "pnpm run -C script start",
    "global": "tsx ./script/build/generate.global.ts",
    "web-api": "tsx script/web-types/vue-docgen.ts",
    "lint:fix": "eslint --cache --fix",
    "lint": "eslint --ext .ts,.js,.jsx,.vue .",
    "version": "standard-version",
    "beta": "pnpm run version --prerelease beta && pnpm run -C script release",
    "release": "pnpm run version && pnpm run -C script release",
    "prepare": "husky",
    "commit": "git add . && git-cz && git push"
  },
  "packageManager": "pnpm@9.13.2",
  "peerDependencies": {
    "@element-plus/icons-vue": ">=2.3.1",
    "@eqian/utils-vue": ">=3.0.0",
    "element-plus": ">=2.7.0",
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@e-plus-ui/element": "workspace:*",
    "@e-plus-ui/icons": "^1.3.1",
    "@e-plus-ui/pure": "workspace:*",
    "@e-plus-ui/styles": "workspace:*",
    "@e-plus-ui/tailwind-config": "workspace:*",
    "@e-plus-ui/utils": "workspace:*",
    "@eqian/utils-vue": "^3.1.2",
    "e-plus-ui": "workspace:*",
    "element-plus": "^2.9.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.81.0",
    "vue": "^3.5.13"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix"
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eqian/eslint-config-preset": "^1.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.1",
    "commitlint-config-cz": "^0.13.3",
    "compare-func": "^2.0.0",
    "consola": "^3.2.3",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "minimist": "^1.2.8",
    "postcss": "^8.4.49",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "5.6.2",
    "vite": "^6.0.1",
    "vue-docgen-api": "^4.79.2",
    "vue-tsc": "^2.1.10"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@eqian/eslint-config-preset/prettier"
}
